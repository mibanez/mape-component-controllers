
-- Add a new worker.
-- Assumes that is executed from the composite solver component.
-- The workers have not monitors.

gcma = deploy-gcma("src/Examples/examples/md5cracker/GCMALocal.xml");
worker = gcm-new("examples.md5cracker.cracker.solver.Worker", $gcma);
set-name($worker, "Generated Worker");
stop($this);
add($this, $worker);
bind($this/child::solver-manager-comp/interface::worker-multicast-itf, $worker/interface::worker-itf);
start($this);
set-value($this/attribute::numberOfWorkers, 2);